<odoo>
    <record id="view_partner_form_saas" model="ir.ui.view">
        <field name="name">res.partner.form.saas</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="inside">
                <group string="Channel">
                    <field name="is_channel_partner"/>
                    <field name="partner_tier"/>
                    <field name="commission_rate"/>
                    <field name="payout_method"/>
                    <field name="iban"/>
                    <field name="bic"/>
                    <field name="vat_id"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
