<odoo>
    <record id="view_tenant_tree" model="ir.ui.view">
        <field name="name">saas.tenant.tree</field>
        <field name="model">saas.tenant</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="partner_id"/>
                <field name="status"/>
                <field name="stage"/>
                <field name="progress"/>
            </tree>
        </field>
    </record>
    <record id="view_tenant_form" model="ir.ui.view">
        <field name="name">saas.tenant.form</field>
        <field name="model">saas.tenant</field>
        <field name="arch" type="xml">
            <form string="Tenant">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="partner_id"/>
                        <field name="plan"/>
                        <field name="region"/>
                        <field name="status"/>
                        <field name="stage"/>
                        <field name="progress"/>
                        <field name="access_url"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <menuitem id="menu_saas_tenant" name="Tenants" parent="saas_partner_channel.menu_saas_channel_root" action="action_saas_tenant"/>
    <record id="action_saas_tenant" model="ir.actions.act_window">
        <field name="name">Tenants</field>
        <field name="res_model">saas.tenant</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
