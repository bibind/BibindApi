<odoo>
    <record id="view_payout_run_tree" model="ir.ui.view">
        <field name="name">saas.payout.run.tree</field>
        <field name="model">saas.payout.run</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="period_start"/>
                <field name="period_end"/>
                <field name="state"/>
            </tree>
        </field>
    </record>
    <record id="action_saas_payout_run" model="ir.actions.act_window">
        <field name="name">Payout Runs</field>
        <field name="res_model">saas.payout.run</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="menu_saas_payout_run" name="Payouts" parent="saas_partner_channel.menu_saas_channel_root" action="action_saas_payout_run"/>
</odoo>
