<odoo>
    <record id="res_partner_demo_registered" model="res.partner">
        <field name="name">Demo Registered Partner</field>
        <field name="is_channel_partner" eval="True"/>
        <field name="partner_tier">registered</field>
        <field name="commission_rate">10</field>
    </record>
    <record id="res_partner_demo_silver" model="res.partner">
        <field name="name">Demo Silver Partner</field>
        <field name="is_channel_partner" eval="True"/>
        <field name="partner_tier">silver</field>
        <field name="commission_rate">15</field>
    </record>
    <record id="saas_tenant_demo_provisioning" model="saas.tenant">
        <field name="name">Demo Provisioning</field>
        <field name="partner_id" ref="res_partner_demo_registered"/>
        <field name="status">provisioning</field>
        <field name="stage">infra</field>
    </record>
    <record id="saas_tenant_demo_ready" model="saas.tenant">
        <field name="name">Demo Ready</field>
        <field name="partner_id" ref="res_partner_demo_silver"/>
        <field name="status">ready</field>
        <field name="stage">ready</field>
        <field name="access_url">https://demo.example.com</field>
    </record>
</odoo>
