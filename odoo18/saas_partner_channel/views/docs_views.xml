<odoo>
    <template id="portal_docs_page" name="Docs">
        <t t-call="portal.portal_layout">
            <div class="container">
                <h2>Documentation</h2>
                <ul class="nav nav-tabs">
                    <li class="nav-item">
                        <a class="nav-link active" href="#">Overview</a>
                    </li>
                </ul>
                <div class="mt-3">
                    <div class="d-flex flex-wrap gap-2">
                        <a class="btn btn-secondary" href="#">Action</a>
                    </div>
                    <ul class="o_portal_timeline list-unstyled mt-3">
                        <li t-foreach="[]" t-as="item" class="mb-2">
                            <span t-esc="item"/>
                        </li>
                    </ul>
                </div>
            </div>
        </t>
    </template>
</odoo>
