<odoo>
    <record id="view_saas_tenant_tree" model="ir.ui.view">
        <field name="name">saas.tenant.tree</field>
        <field name="model">saas.tenant</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="partner_id"/>
                <field name="status"/>
                <field name="stage"/>
                <field name="progress"/>
            </tree>
        </field>
    </record>
    <record id="view_saas_tenant_form" model="ir.ui.view">
        <field name="name">saas.tenant.form</field>
        <field name="model">saas.tenant</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="partner_id"/>
                        <field name="plan"/>
                        <field name="region"/>
                        <field name="admin_email"/>
                        <field name="status"/>
                        <field name="stage"/>
                        <field name="progress"/>
                        <field name="access_url"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
